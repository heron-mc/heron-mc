<contextCollection xmlns="http://schemas.heron-mc.org/hmc/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://schemas.heron-mc.org/hmc/1.0 http://schemas.heron-mc.org/hmc/1.0/heron-map-context.xsd">
	<context>
		<title>RD info</title>
		<wmsLayer>
			<title>Stations [wms]</title>
			<url>{baseUrl}rdinfo/wms?</url>
			<isBaseLayer>false</isBaseLayer>
			<opacity>0.7</opacity>
			<isTransparent>true</isTransparent>
			<isVisible>false</isVisible>
			<maxResolution>53.76</maxResolution>
			<layers>stations</layers>
			<format>image/png</format>
			<featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
			<isAlpha>true</isAlpha>
			<isSingleTile>true</isSingleTile>
		</wmsLayer>
		<wmsLayer>
			<title>Punten [wms]</title>
			<url>{baseUrl}rdinfo/wms?</url>
			<isBaseLayer>false</isBaseLayer>
			<opacity>0.7</opacity>
			<isTransparent>true</isTransparent>
			<isVisible>false</isVisible>
			<layers>punten</layers>
			<format>image/png</format>
			<featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
			<isAlpha>true</isAlpha>
			<isSingleTile>true</isSingleTile>
		</wmsLayer>
	</context>
</contextCollection>