            <maxResolution>1.68</maxResolution>
            <wfsVersion>1.0.0</wfsVersion>
            <wfsFeatureNamespace>http://bagviewer.geonovum.nl</wfsFeatureNamespace>
            <wfsFeatureType>pand</wfsFeatureType>
            <wfsMaxFeatures>1000</wfsMaxFeatures>
            <wfsGeometryElement>geometrie</wfsGeometryElement>
            <styleMap>
                <style>
                    <name>default</name>
                    <property>
                        <name>strokeColor</name>
                        <value>#222222</value>
                    </property>
                    <property>
                        <name>fillColor</name>
                        <value>#eeeeee</value>
                    </property>
                    <property>
                        <name>strokeWidth</name>
