                   xsi:schemaLocation="http://preview.pdok.nl/1.0 config.xsd ">
    <context>
        <title>BAGViewer</title>
        <wmsLayer>
            <title>Panden [wms]</title>
            <url>{baseUrl}bagviewer/wms?</url>
            <isBaseLayer>false</isBaseLayer>
            <opacity>0.7</opacity>
            <isTransparent>true</isTransparent>
            <isVisible>false</isVisible>
            <maxResolution>1.68</maxResolution>
            <layers>pand</layers>
            <format>image/png</format>
            <featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
            <isAlpha>true</isAlpha>
            <isSingleTile>true</isSingleTile>
        </wmsLayer>
        <wfsLayer>
            <title>Panden [wfs]</title>
            <url>{baseUrl}bagviewer/wfs?</url>
