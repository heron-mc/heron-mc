<contextCollection xmlns="http://preview.pdok.nl/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://preview.pdok.nl/1.0 config.xsd ">
	<context>
			<title>Nationaal Wegen Bestand</title>
			<wfsLayer>
				<title>NWB Spoorwegen - Stations [wfs]</title>
				<url>{baseUrl}nwbspoorwegen/wfs?</url>
				<isBaseLayer>false</isBaseLayer>
				<opacity>0.7</opacity>
				<isTransparent>true</isTransparent>
				<isVisible>false</isVisible>
				<wfsVersion>1.0.0</wfsVersion>
				<wfsFeatureNamespace>http://nwbspoorwegen.geonovum.nl</wfsFeatureNamespace>
				<wfsFeatureType>treinstations</wfsFeatureType>
				<wfsMaxFeatures>250</wfsMaxFeatures>
				<wfsGeometryElement>geom</wfsGeometryElement>
				<styleMap>
					<style>
						<name>default</name>
						<property>
							<name>pointRadius</name>
							<value>5</value>
						</property>
						<property>
							<name>strokeColor</name>
							<value>#ffcc66</value>
						</property>
						<property>
							<name>fillColor</name>
							<value>#ff9933</value>
						</property>
						<property>
							<name>graphicZIndex</name>
							<value>1</value>
						</property>
					</style>
					<style>
						<name>select</name>
						<property>
							<name>strokeColor</name>
							<value>#66ccff</value>
						</property>
						<property>
							<name>fillColor</name>
							<value>#3399ff</value>
						</property>
						<property>
							<name>graphicZIndex</name>
							<value>2</value>
						</property>
					</style>
				</styleMap>
			</wfsLayer>
			<wfsLayer>
				<title>NWB Spoorwegen - Spoorvakken [wfs]</title>
				<url>{baseUrl}nwbspoorwegen/wfs?</url>
				<isBaseLayer>false</isBaseLayer>
				<opacity>0.7</opacity>
				<isTransparent>true</isTransparent>
				<isVisible>false</isVisible>
				<wfsVersion>1.0.0</wfsVersion>
				<wfsFeatureNamespace>http://nwbspoorwegen.geonovum.nl</wfsFeatureNamespace>
				<wfsFeatureType>spoorvakken</wfsFeatureType>
				<wfsMaxFeatures>2000</wfsMaxFeatures>
				<wfsGeometryElement>geom</wfsGeometryElement>
				<styleMap>
					<style>
						<name>default</name>
						<property>
							<name>strokeColor</name>
							<value>#ffcc66</value>
						</property>
						<property>
							<name>fillColor</name>
							<value>#ff9933</value>
						</property>
						<property>
							<name>strokeWidth</name>
							<value>2</value>
						</property>
						<property>
							<name>graphicZIndex</name>
							<value>1</value>
						</property>
					</style>
					<style>
						<name>select</name>
						<property>
							<name>strokeColor</name>
							<value>#66ccff</value>
						</property>
						<property>
							<name>fillColor</name>
							<value>#3399ff</value>
						</property>
						<property>
							<name>graphicZIndex</name>
							<value>2</value>
						</property>
					</style>
				</styleMap>
			</wfsLayer>
	</context>
</contextCollection>