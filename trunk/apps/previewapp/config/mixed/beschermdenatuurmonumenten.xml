<contextCollection xmlns="http://preview.pdok.nl/1.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://preview.pdok.nl/1.0 config.xsd ">
    <context>
        <title>Beschermde natuurmonumenten</title>
        <wmsLayer>
            <title>Natuurmonumenten [wms]</title>
            <url>{baseUrl}beschermdenatuurmonumenten/wms?</url>
            <isBaseLayer>false</isBaseLayer>
            <opacity>0.7</opacity>
            <isTransparent>true</isTransparent>
            <isVisible>false</isVisible>
            <layers>beschermdenatuurmonumenten</layers>
            <format>image/png</format>
            <featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
            <isAlpha>true</isAlpha>
            <isSingleTile>true</isSingleTile>
        </wmsLayer>
        <atomLayer>
            <title>Natuurmonumenten [atom]</title>
            <url>{baseUrl}beschermdenatuurmonumenten/atom/beschermdenatuurmonumenten.xml</url>
            <isBaseLayer>false</isBaseLayer>
            <isVisible>false</isVisible>
        </atomLayer>
    </context>
</contextCollection>
