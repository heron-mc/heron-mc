<contextCollection xmlns="http://preview.pdok.nl/1.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://preview.pdok.nl/1.0 config.xsd ">
    <context>
        <title>Weggegevens</title>
        <wmsLayer>
            <title>Aantal rijbanen [wms]</title>
            <url>{baseUrl}weggeg/wms?</url>
            <isBaseLayer>false</isBaseLayer>
            <opacity>0.7</opacity>
            <isTransparent>true</isTransparent>
            <isVisible>false</isVisible>
            <layers>weggegaantalrijbanen</layers>
            <format>image/png</format>
            <featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
            <isAlpha>true</isAlpha>
            <isSingleTile>true</isSingleTile>
        </wmsLayer>
        <wmsLayer>
            <title>Maximum snelheid [wms]</title>
            <url>{baseUrl}weggeg/wms?</url>
            <isBaseLayer>false</isBaseLayer>
            <opacity>0.7</opacity>
            <isTransparent>true</isTransparent>
            <isVisible>false</isVisible>
            <layers>weggegmaximumsnelheden</layers>
            <format>image/png</format>
            <featureInfoFormat>application/vnd.ogc.gml</featureInfoFormat>
            <isAlpha>true</isAlpha>
            <isSingleTile>true</isSingleTile>
        </wmsLayer>
        <atomLayer>
            <title>Weggegevens [atom]</title>
            <url>{baseUrl}weggeg/atom/weggeg.xml</url>
            <isBaseLayer>false</isBaseLayer>
            <isVisible>false</isVisible>
        </atomLayer>
    </context>
</contextCollection>